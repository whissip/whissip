#: ..\..\..\inc\locales\_locale_settings.form.php:139
msgid "m - month; i.e. \"01\" to \"12\""
msgstr ""

#: ..\..\..\inc\locales\_locale_settings.form.php:140
msgid "M - month, textual, 3 letters; i.e. \"Jan\""
msgstr ""

#: ..\..\..\inc\locales\_locale_settings.form.php:141
msgid "n - month without leading zeros; i.e. \"1\" to \"12\""
msgstr ""

#: ..\..\..\inc\locales\_locale_settings.form.php:142
